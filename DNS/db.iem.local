;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA      iem.local. root. iem.local. (
                              3         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      iem.local.
@       IN      A       <IP>
@       IN      AAAA    ::1